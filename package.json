        "@servicenow/now-alert": "orlando",
        "@servicenow/now-toggle": "orlando",
        "@servicenow/ui-effect-graphql": "orlando",
        "sn-translate": "orlando"
    },
    "devDependencies": {
        "@servicenow/cli-archetype-dev": "orlando",
